{
	"name": "linguist-js",
	"version": "1.2.4",
	"description": "Analyse languages used in a folder. Powered by GitHub Linguist, although it doesn't need to be installed.",
	"main": "src/index.js",
	"scripts": {
		"prepublish": "tsc",
		"test": "tsc && node test/test"
	},
	"bin": {
		"linguist": "bin/linguist.js"
	},
	"files": [
		"bin/*.js",
		"src/**/*.js",
		"src/**/*.d.ts"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Nixinova/Linguist.git"
	},
	"keywords": [
		"linguist",
		"languages",
		"language-analysis",
		"language-analyzer"
	],
	"author": "Nixinova (https://nixinova.com)",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Nixinova/Linguist/issues"
	},
	"homepage": "https://github.com/Nixinova/Linguist#readme",
	"dependencies": {
		"glob": "^7.1.7",
		"glob-to-regexp": "^0.4.1",
		"js-yaml": "^4.1.0",
		"node-fetch": "^2.6.1",
		"yargs-parser": "^20.2.9"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/glob-to-regexp": "^0.4.1",
		"@types/js-yaml": "^4.0.2",
		"@types/node": "^16.4.0",
		"@types/node-fetch": "^2.5.11",
		"@types/yargs": "^17.0.2",
		"typescript": "~4.3.5"
	}
}
